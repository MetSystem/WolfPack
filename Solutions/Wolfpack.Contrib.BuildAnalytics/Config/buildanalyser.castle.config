        <ConfigurationName></ConfigurationName>
      </parameters>
    </component>
	
	
   
    <component id="CoverageBuildNotificationBinding" lifestyle="singleton"
                   type="Wolfpack.Core.Interfaces.Entities.BindingConfiguration, Wolfpack.Core.Interfaces">
