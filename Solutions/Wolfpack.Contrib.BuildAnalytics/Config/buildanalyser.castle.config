        <BuildServerUrl></BuildServerUrl>
        <UserId></UserId>
        <Password></Password>
        <UseSsl>false</UseSsl>
        <TrustDuffSslCertificate>false</TrustDuffSslCertificate>
        <ProjectName></ProjectName>
        <ConfigurationName></ConfigurationName>
      </parameters>
    </component>   
